<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aranda.ensiklopedia" version="2.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
    <name>Ensiklopedia Ibu Hamil dan Balita</name>
    <description>
        Aplikasi panduan kesehatan bagi ibu hamil dan balita
    </description>
    <author email="arsoedjono@gmail.com" href="http://its.ac.id">
        Aranda Rizki Soedjono
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
